{
  "name": "@daimo/contract",
  "version": "0.2.7",
  "description": "Daimo contracts, ABIs, and generated Typescript types",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "fetch": "npm run fetch:tokens && npm run fetch:feeds",
    "fetch:tokens": "tsx ./script/gen-tokens.ts",
