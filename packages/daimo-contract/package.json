  "devDependencies": {
    "@pimlico/erc20-paymaster": "^0.1.1",
    "@tsconfig/node20": "^20.1.0",
    "@wagmi/cli": "^1.4.1",
    "tsx": "^4.7.2",
    "typescript": "^5.3.0",
    "userop": "^0.3.2",
    "viem": "^1.19.15"
  },
  "files": [
    "dist/",
    "node_modules/",
    "package.json",
    "README.md"
  ]
}
